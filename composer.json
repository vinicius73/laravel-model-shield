{
    "name": "vinicius73/laravel-model-shield",
    "description": "Validation package for models Laravel 4.*",
    "keywords": ["model", "validation", "validator", "database"],
    "license": "MIT",
    "authors": [{
        "name": "Vinicius Reis",
        "email": "luiz.vinicius73@gmail.com"
    }],
    "require": {
        "php": ">=5.4.0",
        "illuminate/database": "4.*",
        "illuminate/validation": "4.*",
        "illuminate/support": "4.*"
    },
    "require-dev": {
        "zizaco/factory-muff": "dev-master"
    },
    "suggest": {
        "kennedytedesco/validation": "Give more power to your validator."
    },
    "autoload": {
        "psr-0": {
            "Vinicius73\\ModelShield\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}