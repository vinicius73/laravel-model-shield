{
    "name": "vinicius73/laravel-model-shield",
    "description": "Validation package for models Laravel 5+",
    "keywords": ["model", "validation", "validator", "database"],
    "license": "MIT",
    "authors": [{
        "name": "Vinicius Reis",
        "email": "luiz.vinicius73@gmail.com"
    }],
    "require": {
        "php": ">=5.4.0",
        "illuminate/database": "~5",
        "illuminate/validation": "~5",
        "illuminate/support": "~5"
    },
    "suggest": {
        "kennedytedesco/validation": "Give more power to your validator."
    },
    "autoload": {
        "psr-4": {
            "Vinicius73\\ModelShield\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}