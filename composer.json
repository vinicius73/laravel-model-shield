{
    "name": "vinicius73/laravel-model-shield",
    "description": "Validation package for models Laravel 4.1",
    "authors": [{
        "name": "Vinicius Reis",
        "email": "luiz.vinicius73@gmail.com"
    }],
    "require": {
        "php": ">=5.4.0",
        "illuminate/database": "4.*",
        "kennedytedesco/validation": "dev-master"
    },
    "require-dev": {
        "zizaco/factory-muff": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Vinicius73\\ModelShield\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}